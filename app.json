{
  "expo": {
    "name": "gc-client-new",
    "slug": "gc-client-new",
    "version": "1.0.0",
    "scheme": "gigacharger",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sholeka.gcclientnew",
      "config": {
        "googleMapsApiKey": "AIzaSyBkZBxH9wphpgAKxyoEbEUTbhWN862r46c"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.sholeka.gcclientnew",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBWMlk6E5B8zSZui90Ev0MCHjsghCFq55k"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with Gigacharger.",
          "cameraPermission": "The app accesses your camera to let you take photos to share with Gigacharger."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b6b4896c-7aac-4f06-bd28-d5b2b360d3d2"
      }
    }
  }
}
